<?xml version="1.0" encoding="utf-8"?>

<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".BasicDetails.DetailsActivity4"
    >
    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#A6D7FF"
        >




        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#A6D7FF"
            android:layout_marginTop="-15dp"
            android:orientation="vertical"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:background="#A6D7FF"
                android:id="@+id/froglayout"

                >



                <com.airbnb.lottie.LottieAnimationView
                    android:id="@+id/frog1"
                    android:layout_width="0dp"
                    android:layout_height="80dp"
                    android:visibility="visible"
                    app:lottie_autoPlay="true"
                    app:lottie_rawRes="@raw/frog"
                    app:lottie_loop="true"
                    app:lottie_speed="1"

                    android:layout_weight="1"
                    android:contentDescription="@string/imageAccess" />


                <com.airbnb.lottie.LottieAnimationView
                    android:id="@+id/frog2"
                    android:layout_width="0dp"
                    android:layout_height="80dp"                    android:visibility="visible"

                    app:lottie_autoPlay="false"
                    app:lottie_rawRes="@raw/frog"
                    app:lottie_loop="true"
                    app:lottie_speed="1"

                    android:layout_weight="1"

                    android:contentDescription="@string/imageAccess" />

                <com.airbnb.lottie.LottieAnimationView
                    android:id="@+id/frog3"
                    android:layout_width="0dp"
                    android:layout_height="80dp"                    android:visibility="visible"
                    android:layout_weight="1"
                    app:lottie_autoPlay="false"
                    app:lottie_rawRes="@raw/frog"
                    app:lottie_loop="true"
                    app:lottie_speed="1"


                    android:contentDescription="@string/imageAccess" />

                <com.airbnb.lottie.LottieAnimationView
                    android:id="@+id/frog4"
                    android:layout_width="0dp"
                    android:layout_height="80dp"                    android:visibility="visible"
                    app:lottie_autoPlay="false"
                    app:lottie_rawRes="@raw/frog"
                    app:lottie_loop="true"
                    app:lottie_speed="1"


                    android:layout_weight="1"

                    android:contentDescription="@string/imageAccess" />


            </LinearLayout>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginHorizontal="20dp"
                android:textAlignment="center"
                android:fontFamily="@font/bariol_regular"
                android:text="@string/you_are_at_last_step_to_setup_your_profile"
                android:layout_gravity="center"
                android:textColor="@color/colorPrimaryDark"
                android:textSize="30sp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"

                android:paddingLeft="@dimen/horizontal_dashboard"
                android:paddingTop="@dimen/fab_margin"
                android:paddingRight="@dimen/horizontal_dashboard"


                android:weightSum="100">

                <com.airbnb.lottie.LottieAnimationView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:visibility="visible"
                    app:lottie_autoPlay="true"
                    app:lottie_rawRes="@raw/announcement"
                    app:lottie_loop="true"
                    app:lottie_speed="1"

                    android:layout_weight="60"
                    android:contentDescription="@string/imageAccess" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="30dp"
                    android:layout_weight="40"
                    android:fontFamily="@font/bariol_bold"
                    android:text="@string/what_qualities_do_you_like_in_person"
                    android:textColor="@color/colorPrimaryDark"
                    android:textSize="30sp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/horizontal_dashboard"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/normalPadding"

                android:layout_marginRight="@dimen/horizontal_dashboard">
                <com.google.android.material.chip.Chip
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/chip1"
                    android:layout_weight="1"
                    android:checkable="true"
                    android:layout_marginEnd="2dp"
                    android:text="@string/charming"/>
                <com.google.android.material.chip.Chip
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/chip2"
                    android:layout_weight="1"
                    android:checkable="true"
                    android:layout_marginEnd="2dp"
                    android:text="@string/calm"/>
                <com.google.android.material.chip.Chip
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:id="@+id/chip3"
                    android:checkable="true"
                    android:layout_marginEnd="2dp"
                    android:text="@string/honesty"/>


            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/horizontal_dashboard"
                android:orientation="horizontal"


                android:layout_marginRight="@dimen/horizontal_dashboard">
                <com.google.android.material.chip.Chip
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/chip4"
                    android:layout_weight="0.8"
                    android:checkable="true"
                    android:layout_marginEnd="2dp"
                    android:text="@string/confidence"/>
                <com.google.android.material.chip.Chip
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/chip5"
                    android:layout_weight="1"
                    android:checkable="true"
                    android:layout_marginEnd="2dp"
                    android:text="@string/friendly"/>
                <com.google.android.material.chip.Chip
                    android:layout_width="0dp"
                    android:layout_weight="0.7"
                    android:layout_height="wrap_content"
                    android:id="@+id/chip6"
                    android:checkable="true"
                    android:layout_marginEnd="2dp"
                    android:text="@string/social"/>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/horizontal_dashboard"
                android:orientation="horizontal"


                android:layout_marginRight="@dimen/horizontal_dashboard">
                <com.google.android.material.chip.Chip
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/chip7"
                    android:layout_weight="0.6"
                    android:checkable="true"
                    android:layout_marginEnd="2dp"
                    android:text="@string/loyal"/>
                <com.google.android.material.chip.Chip
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/chip8"
                    android:layout_weight="0.5"
                    android:checkable="true"
                    android:layout_marginEnd="2dp"
                    android:text="@string/kind"/>
                <com.google.android.material.chip.Chip
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:id="@+id/chip9"
                    android:checkable="true"
                    android:layout_marginEnd="2dp"
                    android:text="@string/hard_working"/>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/horizontal_dashboard"
                android:orientation="horizontal"


                android:layout_marginRight="@dimen/horizontal_dashboard">
                <com.google.android.material.chip.Chip
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/chip10"
                    android:checkable="true"
                    android:layout_marginEnd="2dp"
                    android:text="@string/passionate"/>

                <com.google.android.material.chip.Chip
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/chip11"
                    android:checkable="true"
                    android:layout_marginEnd="2dp"
                    android:text="@string/beautiful"/>


            </LinearLayout>

            <EditText
                android:id="@+id/etQualityLike"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="@dimen/horizontal_dashboard"
                android:layout_marginTop="@dimen/normalPadding"
                android:singleLine="false"
                android:inputType="textCapSentences|textMultiLine"
                android:layout_marginRight="@dimen/horizontal_dashboard"
                android:autofillHints="username"
                android:drawablePadding="@dimen/drawable_left"
                android:fontFamily="@font/bariol_regular"
                android:gravity="center_vertical"
                android:hint="@string/other_s_if_any"
                android:background="@drawable/container_search"
                android:paddingLeft="@dimen/normalPadding"
                android:paddingTop="@dimen/vertical_search"
                android:paddingRight="@dimen/normalPadding"
                android:textStyle="bold"
                android:paddingBottom="@dimen/vertical_search"
                android:textColor="?attr/colorPrimaryDark"
                android:textColorHint="@color/color_hint"
                android:textSize="22sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginTop="30dp"
                android:layout_weight="40"
                android:layout_marginHorizontal="29dp"
                android:fontFamily="@font/bariol_bold"
                android:text="@string/what_qualities_do_you_dislike_in_person"
                android:textColor="@color/colorPrimaryDark"
                android:textSize="30sp" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/horizontal_dashboard"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/normalPadding"

                android:layout_marginRight="@dimen/horizontal_dashboard">

                <com.google.android.material.chip.Chip
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/chip1d"
                    android:layout_weight="1"
                    android:checkable="true"
                    android:layout_marginEnd="2dp"
                    android:text="@string/self_centered"/>
                <com.google.android.material.chip.Chip
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:id="@+id/chip2d"
                    android:checkable="true"
                    android:layout_marginEnd="2dp"
                    android:text="@string/negative"/>


            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/horizontal_dashboard"
                android:orientation="horizontal"


                android:layout_marginRight="@dimen/horizontal_dashboard">
                <com.google.android.material.chip.Chip
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/chip3d"
                    android:layout_weight="0.8"
                    android:checkable="true"
                    android:layout_marginEnd="2dp"
                    android:text="@string/judgemental"/>

                <com.google.android.material.chip.Chip
                    android:layout_width="0dp"
                    android:layout_weight="0.7"
                    android:layout_height="wrap_content"
                    android:id="@+id/chip4d"
                    android:checkable="true"
                    android:layout_marginEnd="2dp"
                    android:text="@string/dishonest"/>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/horizontal_dashboard"
                android:orientation="horizontal"


                android:layout_marginRight="@dimen/horizontal_dashboard">
                <com.google.android.material.chip.Chip
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/chip5d"
                    android:layout_weight="0.6"
                    android:checkable="true"
                    android:layout_marginEnd="2dp"
                    android:text="@string/selfish"/>

                <com.google.android.material.chip.Chip
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:id="@+id/chip6d"
                    android:checkable="true"
                    android:layout_marginEnd="2dp"
                    android:text="@string/arrogance"/>


            </LinearLayout>

            <EditText
                android:id="@+id/etQualityDislike"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="@dimen/horizontal_dashboard"
                android:layout_marginTop="@dimen/normalPadding"
                android:singleLine="false"
                android:inputType="textCapSentences|textMultiLine"
                android:layout_marginRight="@dimen/horizontal_dashboard"
                android:autofillHints="username"
                android:drawablePadding="@dimen/drawable_left"
                android:fontFamily="@font/bariol_regular"
                android:gravity="center_vertical"
                android:hint="@string/other_s_if_anya"
                android:layout_marginBottom="5dp"
                android:background="@drawable/container_search"
                android:paddingLeft="@dimen/normalPadding"
                android:paddingTop="@dimen/vertical_search"
                android:paddingRight="@dimen/normalPadding"
                android:textStyle="bold"
                android:paddingBottom="@dimen/vertical_search"
                android:textColor="?attr/colorPrimaryDark"
                android:textColorHint="@color/color_hint"
                android:textSize="22sp" />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:orientation="horizontal"
                android:layout_marginTop="30dp"
                android:paddingHorizontal="30dp"
                tools:ignore="RtlHardcoded">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_marginBottom="@dimen/margin_20_dp"
                    android:layout_height="match_parent"
                    tools:ignore="UselessParent">

                    <ImageView
                        android:id="@+id/tvPrevious"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_gravity="left"
                        android:src="@drawable/prev"
                        android:contentDescription="@string/todo" />

                    <ImageView
                        android:id="@+id/tvNext"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_alignParentRight="true"
                        android:src="@drawable/next"
                        android:contentDescription="@string/todo" />


                </RelativeLayout>



            </LinearLayout>











        </LinearLayout>

    </androidx.core.widget.NestedScrollView>

</FrameLayout>